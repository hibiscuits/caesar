@FFI("C") extern fn printf(fmt: Byte&, ...) -> Int32
@FFI("C") extern fn atoi(str: Byte&) -> Int32

struct S
	a: Int32
	b: Int32

fn main(argc: UInt64, argv: Byte&&)
	if argc != 3
		printf("Usage: %s <number> <number>\n", argv^)
	
	let s = S
		a: atoi(argv[8])
		b: atoi(argv[16])
	
	printf("%d, %d\n", s.a+1, s.b+1)
